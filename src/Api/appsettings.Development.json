{
  "ConnectionStrings": {
    "DbConnection": "your-connectionstring-here",
    "DbReadOnlyConnection": "your-connectionstring-here"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "CoreSettings": {
    "Name": "Dotnet Sample Api",
    "Version": "v0.0.1",
    "Seq": {
      "IsActive": "false",
      "SeqApiKey": "seq-api-key",
      "SeqServerUrl": "https://www.seq-url.com"
    },
    "Cache": {
      "IsActive": "true",
      "RedisInstanceName": "redis-instance-name:",
      "RedisConfiguration": ""
    },
    "Localization": {
      "HeaderName": "APICulture",
      "DefaultCulture": "en-GB",
      "SupportedCultures": [
        "tr-TR",
        "en-US",
        "en-GB"
      ]
    },
    "Auth": {
      "System": "DotnetSampleApi",
      "JwtSecret": "!!!-updateWithYourCustomJWTKey-1234-!!-Dev",
      "TokenDuration": 60
    },
    "Sentry": {
      "IsActive": false,
      "Dsn": "https://sentry-sentry-url",
      "Debug": false
    },
    "Datadog": {
      "IsActive": false,
      "ServiceName": "datadog-api-name"
    }
  }
}
